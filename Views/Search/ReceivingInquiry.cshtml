                <label for="sale-uom-name" class="control-label text-primary">Selling UOM:</label>
                <input type="text" id="sale-uom-name" name="sale-uom-name" class="form-control" value="@(Model.ProductInfo.SaleUomName.ToUpper() == "INDIVIDUALLY" ? "EACH" : Model.ProductInfo.SaleUomName.ToUpper().Replace("PER", "").Trim())" readonly="">
            </div>

            <div class="col-sm-8 form-group">
                <div class="md-spacer"></div>
            </div>

            <div class="col-sm-8 form-group">
                <div class="md-spacer"></div>
            </div>

