@model DataEntry.Models.ProductModel

<div class="container">
    @if (Model.ProductInfo == null)
    {
        <div class="row">
            <div class="col-md-12" style="padding-top: 40px;">
                <h2 style="text-align: center;">Item number @Model.ItemNumber is not in Insite.</h2>
            </div>
        </div>
    }
    else
    {
        <div class="row">
            <div class="col-xs-12">
                <div class="spacer-md"></div>
            </div>
            <div class="col-sm-12 text-center">
                <h1 class="mt-0">@Model.ProductInfo.ShortDescription</h1>
                <div class="spacer-md"></div>
            </div>
            <div class="col-sm-4">
                <img src="https://products.riogrande.com/products/@(Model.ItemNumber).jpg" alt="Alternate Text" class="img-responsive img-thumbnail">
            </div>
            <div class="col-sm-8">
                <div class="row">
                    <div class="col-sm-6 form-group">
                        <label for="item-number" class="control-label text-primary">Item number:</label>
                        <input type="text" id="item-number" name="item-number" class="form-control" value="@Model.ItemNumber" readonly="">
                    </div>
                    <div class="col-sm-6 form-group">
                        <label for="product-manager" class="control-label text-primary">Product Manager:</label>
                        <input type="text" id="product-manager" name="product-manager" class="form-control" value="@Model.ProductInfo.ProductManager" readonly="">
                    </div>
                    <div class="col-sm-6 form-group">
                        <label for="supply-manager" class="control-label text-primary">Supply Manager:</label>
                        <input type="text" id="supply-manager" name="supply-manager" class="form-control" value="@Model.ProductInfo.SupplyManager" readonly="">
                    </div>
                    @*
                        <div class="col-sm-6 form-group">
                            <label for="category" class="control-label text-primary">Control Buyer:</label>
                            <input type="text" id="category" name="category" class="form-control" value="@Model.ProductInfo.GroupDescription" readonly="">
                        </div>
                    *@
                </div>
            </div>
            <div class="spacer-md"></div>

            @if (Model.ProductAttributes != null && !string.IsNullOrWhiteSpace(Model.ProductAttributes.CountryOfOrigin))
            {
                <div class="col-sm-4 form-group">
                    <label for="country-origin" class="control-label text-primary">Country of origin:</label>
                    <input type="text" id="country-origin" name="country-origin" class="form-control" value="@Model.ProductAttributes.CountryOfOrigin" readonly="">
                </div>
            }

